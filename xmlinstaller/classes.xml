<?xml version="1.0" encoding="UTF-8"?>
<eZXMLImporter>
	<CreateClass>
		<ContentClass identifier="article" classExistAction="replace"
			isContainer="true" objectNamePattern="&lt;title&gt;" urlAliasPattern="&lt;seo|title&gt;">
			<Names fre-FR="Article"></Names>
			<Groups>
				<Group name="Contenu" />
			</Groups>
			<Attributes>
				<Attribute identifier="title" datatype="ezstring"
					required="true" searchable="true">
					<Names fre-FR="Titre" />
					<DatatypeParameters>
						<max-length>255</max-length>
						<default-string></default-string>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="long_title" datatype="ezstring"
					searchable="true">
					<Names fre-FR="Titre long" />
					<DatatypeParameters>
						<max-length>255</max-length>
						<default-string></default-string>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="accroche" datatype="ezxmltext"
					searchable="true">
					<Names fre-FR="Accroche" />
					<DatatypeParameters>
						<text-column-count>5</text-column-count>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="body" datatype="ezxmltext"
					searchable="true">
					<Names fre-FR="Contenu" />
					<DatatypeParameters>
						<text-column-count>5</text-column-count>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="visuels" datatype="ezobjectrelationlist"
					translatable="false">
					<Names fre-FR="Visuels" />
					<DatatypeParameters>
						<default-placement node-id="51" />
						<selection-type>0</selection-type>
						<type>2</type>
						<allowed-class>
							<contentclass-identifier>image</contentclass-identifier>
							<contentclass-identifier>video</contentclass-identifier>
						</allowed-class>
						<class-constraints>
							<class-constraint class-identifier="image" />
							<class-constraint class-identifier="video" />
						</class-constraints>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="date_publication" datatype="ezdatetime"
					searchable="true" translatable="false">
					<Names fre-FR="Date de publication" />
					<DatatypeParameters>
						<default-value>current-date</default-value>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="date_fin_publication" datatype="ezdatetime"
					searchable="true" translatable="false">
					<Names fre-FR="Date de fin de publication" />
					<DatatypeParameters>
						<default-value>empty</default-value>
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="in_menu" datatype="ezboolean"
					searchable="true" translatable="false">
					<Names fre-FR="Visible dans le(s) menu(s) ?" />
					<DatatypeParameters>
						<default-value is-set="true" />
					</DatatypeParameters>
				</Attribute>
				<Attribute identifier="seo" datatype="xrowmetadata">
					<Names fre-FR="Référencement" />
					<DatatypeParameters></DatatypeParameters>
				</Attribute>
			</Attributes>
		</ContentClass>
