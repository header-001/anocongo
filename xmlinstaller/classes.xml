		<ContentClass identifier="image" classExistAction="replace"
			objectNamePattern="&lt;name|image&gt;" urlAliasPattern="&lt;name|image&gt;">
			<Names fre-FR="Image"></Names>
			<Groups>
				<Group name="Media" />
			</Groups>
			<Attributes>
				<Attribute identifier="name" datatype="ezstring" required="true"
